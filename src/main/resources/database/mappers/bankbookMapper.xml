<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.epic.s1.bankbook.BankbookDAO">
  
  	<insert id="setInsert" parameterType="com.epic.s1.bankbook.BankbookDTO">
  		insert into bankbook (bookNumber, bookName, bookRate, bookSale)
  		values (bankbook_seq.nextval, #{bookName}, #{bookRate}, #{bookSale})
  	</insert>
  	
  	
  	
  	<select id="getList" resultType="com.epic.s1.bankbook.BankbookDTO">
  		SELECT * FROM BANKBOOK
  	</select>
  	
  	<select id="getSelect" parameterType="com.epic.s1.bankbook.BankbookDTO" resultType="com.epic.s1.bankbook.BankbookDTO">
  		SELECT * FROM BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
  	</select>

  
  </mapper>